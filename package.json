    "@types/jest": "^29.4.0",
    "@types/react": "~18.2.14",
    "@types/react-native": "~0.70.6",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^19.0.4",
    "expo": "^49.0.0",
    "expo-dev-client": "^3.3.6",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "jest": "^29.2.1",
    "jest-expo": "~49.0.0",
    "mobx": "^6.8.0",
    "mobx-react": "^7.6.0",
    "mobx-react-lite": "^3.4.0",
    "prettier": "^2.8.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
